{
  "app_id": "wanimate",
  "version": "v1",
  "workflow_path": "./workflow.json",
  "provider": "runpod",
  "gpu_profile": "A10G",
  "preferred_gpu_types": [
    "NVIDIA RTX PRO 6000"
  ],
  "regions": [
    "US"
  ],
  "env": {
    "COMFY_HEADLESS": "1",
    "COMFY_ENDPOINTS_API_KEY": "demo-change-me"
  },
  "endpoint": {
    "name": "run",
    "mode": "async",
    "auth_mode": "api_key",
    "timeout_seconds": 300,
    "max_payload_mb": 10
  },
  "cache_policy": {
    "watch_paths": [
      "/opt/comfy/models"
    ],
    "min_file_size_mb": 100,
    "symlink_targets": [
      "/opt/comfy/models"
    ]
  },
  "compute_policy": {
    "min_vram_gb": 64,
    "gpu_count": 1
  },
  "build": {
    "comfy_version": "0.3.26",
    "image_repository": "ghcr.io/matyoung89/comfy-endpoints-golden",
    "base_image_repository": "ghcr.io/matyoung89/comfy-endpoints-comfybase",
    "base_dockerfile_path": "docker/Dockerfile.comfybase",
    "dockerfile_path": "docker/Dockerfile.golden",
    "build_context": ".",
    "base_build_context": ".",
    "container_registry_auth_id": "",
    "plugins": [
      {
        "repo": "https://github.com/comfyanonymous/ComfyUI",
        "ref": "master"
      },
      {
        "repo": "https://github.com/Comfy-Org/ComfyUI-Manager",
        "ref": "main"
      }
    ]
  },
  "artifacts": [
    {
      "kind": "custom_node",
      "source_url": "https://github.com/kijai/ComfyUI-WanVideoWrapper",
      "ref": "main",
      "match": "ComfyUI-WanVideoWrapper",
      "target_subdir": "custom_nodes",
      "target_path": "ComfyUI-WanVideoWrapper",
      "provides": [
        "WanVideoVAELoader",
        "WanVideoModelLoader",
        "WanVideoAnimateEmbeds",
        "WanVideoSampler",
        "WanVideoDecode"
      ]
    },
    {
      "match": "wanvideo/Wan2_1_VAE_bf16.safetensors",
      "source_url": "https://huggingface.co/Kijai/WanVideo_comfy/resolve/main/Wan2_1_VAE_bf16.safetensors",
      "target_subdir": "vae",
      "target_path": "wanvideo/Wan2_1_VAE_bf16.safetensors"
    },
    {
      "match": "WanVideo/2_2/Wan2_2-Animate-14B_fp8_e4m3fn_scaled_KJ.safetensors",
      "source_url": "https://huggingface.co/Kijai/WanVideo_comfy_fp8_scaled/resolve/main/Wan22Animate/Wan2_2-Animate-14B_fp8_e4m3fn_scaled_KJ.safetensors",
      "target_subdir": "diffusion_models",
      "target_path": "WanVideo/2_2/Wan2_2-Animate-14B_fp8_e4m3fn_scaled_KJ.safetensors"
    }
  ]
}
